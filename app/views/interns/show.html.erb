<div class="page-header">
  <%= link_to interns_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_intern_path(@intern), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show intern</h1>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @intern.title %></dd>

  <dt>Starttime:</dt>
  <dd><%= @intern.starttime %></dd>

  <dt>Endtime:</dt>
  <dd><%= @intern.endtime %></dd>

  <dt>Description:</dt>
  <dd><%= @intern.description %></dd>

  <dt>Url:</dt>
  <dd><%= @intern.url %></dd>

</dl>
